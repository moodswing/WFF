  <emailNotificationTemplates>
    <emailNotificationTemplate>
      <templateId>MAIL_ON_COMPLETION</templateId>
      <recipient>field(TransportCoordinatorEmail),field(createdByEmail)</recipient>
      <subject>Transport Request: Completed</subject>
      <body>
        <![CDATA[
        Please note that I have completed the following transport request.

        Requested by : field(createdBy)
        Date         : DateTime()
        Start Time   : field(ActualStartTime)
        End Time     : field(ActualEndTime)
        Description  : field(RequestDescription)

        Link  : Link()

        Regards,

        User().
        ]]>
      </body>
    </emailNotificationTemplate>
    <emailNotificationTemplate>
      <templateId>MAIL_TO_REQUESTOR_FOR_REVISION</templateId>
      <recipient>field(createdByEmail)</recipient>
      <subject>Transport Request: Revision Required</subject>
      <body>
        <![CDATA[
          Dear field(createdBy),

          This request for transport has been sent back to you for revision. 

          Remarks   : Comments()
          Requestor : field(createdBy)
          Date      : DateTime()
          Time      : field(ProposedStartTime)
          Link      : Link()


          Regards,

          User()
        ]]>
      </body>
    </emailNotificationTemplate>
    <emailNotificationTemplate>
      <templateId>MAIL_TO_DRIVER_ON_ASSIGNMENT</templateId>
      <recipient>emailField(Driver)</recipient>
      <subject>Transport Request: Assignment</subject>
      <body>
        <![CDATA[
          Dear field(Driver)

          Please note that the following transport request has been assigned to you.

          Requestor      : field(createdBy)
          Contact Person : field(ContactPerson)
          Date           : DateTime()
          Time           : field(ProposedStartTime)

          Link  : Link()

          Regards,

          User().
        ]]>
      </body>
    </emailNotificationTemplate>
    <emailNotificationTemplate>
      <templateId>MAIL_TO_REQUESTOR_ON_ASSIGNMENT</templateId>
      <recipient>emailRole(Coordinators)</recipient>
      <subject>Transport Request: Assigned</subject>
      <body>
        <![CDATA[
          Please note that the following transport request has been assigned to the driver indicated.

          Requestor : field(createdBy)
          Date      : DateTime()
          Time      : field(ProposedStartTime)
          Driver    : field(Driver)

          Link   : Link()

          Regards,

          User().
        ]]>
      </body>
    </emailNotificationTemplate>
    <emailNotificationTemplate>
      <templateId>MAIL_ON_SUBMIT</templateId>
      <recipient>emailRole(Coordinators)</recipient>
      <subject>Transport Request: Submitted</subject>
      <body>
        <![CDATA[
          Please note that I have submitted the following transport request.

          Requestor : field(createdBy)
          Date      : DateTime()
          Time      : field(ProposedStartTime)

          Link  : Link()

          Regards,

          User().
        ]]>
      </body>
    </emailNotificationTemplate>
  </emailNotificationTemplates>
