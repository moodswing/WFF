  <statuses>
    <status>
      <name>COMPLETED</name>
      <displayName>Completed</displayName>
      <userAssigned>field(Driver)</userAssigned>
      <order>4</order>
    </status>
    <status>
      <name>ASSIGNED</name>
      <displayName>Assigned</displayName>
      <userAssigned>field(Driver)</userAssigned>
      <editableFields>ActualStartTime,ActualEndTime,CompletionRemarks,TotalGoodsWeight</editableFields>
      <order>3</order>
      <actions>
        <action>
          <name>ADD_COMMENTS</name>
          <displayName>Add Comments</displayName>
          <targetStatus>ASSIGNED</targetStatus>
          <confirmationText>Add Comments ?</confirmationText>
          <commentRequired>true</commentRequired>
          <addCommentToList>Comments</addCommentToList>
        </action>
        <action>
          <name>MARK_COMPLETE</name>
          <displayName>Mark Complete</displayName>
          <targetStatus>COMPLETED</targetStatus>
          <sendEmailNotification>MAIL_ON_COMPLETION</sendEmailNotification>
          <confirmationText>Complete request ?</confirmationText>
          <historyText>Request has been completed.</historyText>
          <requiredFields>ActualStartTime,ActualEndTime</requiredFields>
        </action>
      </actions>
    </status>
    <status>
      <name>AWAITING_ASSIGNMENT</name>
      <displayName>Awaiting Assignment</displayName>
      <userAssigned>field(TransportCoordinator)</userAssigned>
      <editableFields>DestinationAddress,RequestDescription,ProposedDate,ProposedStartTime,ProposedEndTime,DetailedDescription,PassengerCategory,NumberOfPassengers,PassengerNames,PackageTypeCorrespondenceSpecify,PackageTypeCorrespondence,NumberOfItems,PackageTypeBooks,PackageTypeDocuments,Priority,TypeOfTransport,TypeOfGoods,ContactPerson,ContactPhoneNumber,ReturnTripRequired,Driver,Vehicle,Instructions,Attachments</editableFields>
      <hiddenFields>Comments,ActualStartTime,ActualEndTime,CompletionRemarks,TotalGoodsWeight</hiddenFields>
      <order>2</order>
      <actions>
        <action>
          <name>ASSIGN</name>
          <displayName>Assign to Driver</displayName>
          <targetStatus>ASSIGNED</targetStatus>
          <sendEmailNotification>MAIL_TO_DRIVER_ON_ASSIGNMENT,MAIL_TO_REQUESTOR_ON_ASSIGNMENT</sendEmailNotification>
          <confirmationText>Assign to driver (field(Driver)) ?</confirmationText>
          <historyText>Assigned to driver (field(Driver))</historyText>
          <commentRequired>false</commentRequired>
          <requiredFields>Driver,Vehicle</requiredFields>
        </action>
        <action>
          <name>SEND_BACK_TO_CREATOR_FOR_REVISION</name>
          <displayName>Send Back to Creator for Revision</displayName>
          <targetStatus>NEW</targetStatus>
          <sendEmailNotification>MAIL_TO_REQUESTOR_FOR_REVISION</sendEmailNotification>
          <confirmationText>Send back to creator  (field(createdBy)) for revision ?</confirmationText>
          <historyText>Sent back to the requestor for revision.</historyText>
          <commentRequired>true</commentRequired>
        </action>
      </actions>
    </status>
    <status>
      <name>SUBMITTED</name>
      <displayName>Submitted</displayName>
      <userAssigned>role(Coordinators)</userAssigned>
      <editableFields>TransportCoordinator</editableFields>
      <hiddenFields>Driver,Vehicle,Instructions,Comments,ActualStartTime,ActualEndTime,CompletionRemarks,TotalGoodsWeight</hiddenFields>
      <order>1</order>
      <actions>
        <action>
          <name>CHECK_OUT_FROM_INBOX</name>
          <displayName>Check-out from Inbox</displayName>
          <targetStatus>AWAITING_ASSIGNMENT</targetStatus>
          <confirmationText>Check-out from Inbox ?</confirmationText>
          <historyText>Request checked-out for processing by transport coordinator (User())</historyText>
          <commentRequired>false</commentRequired>
          <jsFunction>setFieldValues([{ "target": "TransportCoordinator", "dataField": "currentUser" }, { "target": "TransportCoordinatorEmail", "dataField": "currentUserEmail" }])</jsFunction>
        </action>
      </actions>
    </status>
    <status>
      <name>NEW</name>
      <displayName>New</displayName>
      <userAssigned>field(createdBy)</userAssigned>
